		"@typescript-eslint/parser": "^4.31.2",
		"eslint-config-marine": "^9.0.6",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-prettier": "^4.0.0",
		"prettier": "^2.4.1",
		"tsconfig-paths": "^3.9.0",
		"typescript": "~4.4.3"
