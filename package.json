{
	"name": "baconbot",
	"private": true,
	"version": "1.0.0",
	"description": "The BaconBot Discord.js bot used in The Fryer's Discord Server.",
	"scripts": {
		"start": "npm run build && node -r tsconfig-paths/register dist/bot",
